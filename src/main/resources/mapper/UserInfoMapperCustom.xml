<mapper namespace="com.gsy.luckymoney.mapper.UserInfoMapperCustom" >
  <select id="listByMine" resultType="java.util.HashMap" parameterType="java.util.Map">
    SELECT ID,USERNAME,PASSWORD,USERTYPE FROM
    user_info t where t.id=#{id}
